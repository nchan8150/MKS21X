public class Barcode implements Comparable<Barcode>{
    String zip;

    public Barcode (String zip) {
	this.zip = zip;
    }

    public String getZip() {
	return zip;
    }

    public String getCode() {
	String[] a = ["||:::", ":::||", "::|:|", "::||:", ":|::|", ":|:|:", ":||::", "|:::|", "|::|:", "|:|::"];
	String ans  = "|";
	for (int x = 0; x < zip.length(); x++) {
	    ans += a[zip.charAt(x)];
	}
	return ans + a[checkSum()] + "|";
    }

